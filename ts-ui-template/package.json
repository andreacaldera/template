{
  "name": "ipanda",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=8.11"
  },
  "main": "./src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/src/index.js",
    "dev": "WATCH_MODE=true nodemon --exec 'ts-node --files --project tsconfig.json' src/index.ts",
    "local": "NODE_ENV=dev WATCH_MODE=true nodemon --exec 'ts-node --files --project tsconfig.json' src/index.ts",
    "build": "if [ \"$NODE_ENV\" = \"dev\" ]; then npm run build:dev ; else npm run build:prod; fi",
    "build:dev": "tsc --project tsconfig.production.json && webpack --config=webpack.config.js --progress",
    "build:prod": "tsc --project tsconfig.production.json && NODE_ENV=production webpack --config=webpack.production.config.js --progress",
    "clean": "rm -rf cache dist tmp ts-node",
    "pretest": "yarn lint",
    "test": "ENV_NAME=test jest",
    "ts": "jest --silent --coverage",
    "test:watch": "yarn test --watch",
    "test:types": "tsc --noEmit",
    "test:coverage": "yarn test --coverage",
    "schema": "typescript-json-schema --strictNullChecks --required",
    "t:sw": "yarn test --watch --silent --verbose",
    "lint": "tslint --project tsconfig.json '{src,test}/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix --project tsconfig.json '{src,test}/**/*.{ts,tsx}'",
    "format": "prettier --write '{src,test}/**/*.{js,ts,tsx,json,css,md}'",
    "stats": "webpack --profile --json --config=webpack.production.config.js > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/aceo/instashop.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/aceo/instashop/issues"
  },
  "homepage": "https://gitlab.com/aceo/instashop",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test && yarn build:prod"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
